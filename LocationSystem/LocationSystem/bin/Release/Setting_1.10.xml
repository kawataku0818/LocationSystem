	<!-- 加重移動平均設定-->
	<weightedmovingaveragelist>
		<weightedmovingaverage>
			<!-- 0 〜 100 を設定する-->
			<!-- 作業者の移動平均回数-->
	    	<usergroupcount>10</usergroupcount>
			<!-- 重機の移動平均回数-->
	    	<machinegroupcount>100</machinegroupcount>
		</weightedmovingaverage>
	</weightedmovingaveragelist>

	<!-- XY固定設定-->
	<machinelist>
		<!-- 重機１-->
		<machine no="1">
		    <!-- LockEnableXが1のときX座標は固定されます -->
	    	<LockEnableX>0</LockEnableX>
	    	<!-- 固定するX座標を設定 -->
		    <LockPositionX>100.0</LockPositionX>
		    <!-- LockEnableYが1のときY座標は固定されます -->
		    <LockEnableY>1</LockEnableY>
		    <!-- 固定するY座標を設定 -->
		    <LockPositionY>20.0</LockPositionY>
		</machine>
		<machine no="2">
		    <LockEnableX>1</LockEnableX>
